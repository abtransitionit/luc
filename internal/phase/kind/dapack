package kind

import (
	"github.com/abtransitionit/luc/pkg/pipeline/gocli"
	"github.com/abtransitionit/luc/pkg/logx"
)

const DapackDescription = "provision needed dnfapt packages and repositories"

func dapack(arg ...string) (string, error) {
	logx.L.Info(DapackDescription)
	// Launch the pipeline attach to this phase
	gocli.RunPipeline()
	// on SUCCESS
	return "", nil
}
