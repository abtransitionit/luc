package oservice

import (
	"github.com/abtransitionit/luc/pkg/logx"
	"github.com/spf13/cobra"
)

// Description
var fileSDesc = "manage file services (aka. unit files)."
var fileLDesc = fileSDesc + `xxx`

// provision Command
var fileCmd = &cobra.Command{
	Use:   "file",
	Short: fileSDesc,
	Long:  fileLDesc,
	Run: func(cmd *cobra.Command, args []string) {
		logx.L.Infof("%s", fileSDesc)
		cmd.Help()
	},
}

func init() {
	fileCmd.AddCommand(cFileCmd)
}
